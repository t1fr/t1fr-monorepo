{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@app/battle-schedule/(.*)": "<rootDir>/../libs/battle-schedule/src/$1",
    "@lib/battle-schedule": "<rootDir>/../libs/battle-schedule/src",
    "@app/analysis/(.*)": "<rootDir>/../libs/analysis/src/$1",
    "@lib/analysis": "<rootDir>/../libs/analysis/src",
    "@app/ddd-types/(.*)": "<rootDir>/../libs/ddd-types/src/$1",
    "@lib/ddd-types": "<rootDir>/../libs/ddd-types/src"
  }
}